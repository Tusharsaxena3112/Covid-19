r{\r\n    background-color: turquoise;\r\n    color:black;\r\n    transition:.4s;\r\n}\r\n\r\n\r\n.input {\r\n    text-align: center;\r\n    margin: 50px;\r\n}\r\n\r\n.input input {\r\n    size: 100px;\r\n    height: 5vh;\r\n    font-size: 30px;\r\n    box-shadow: 5px 5px 5px 5px grey;\r\n}\r\n\r\n.country-search{\r\n    position: relative;\r\n    margin-top:20px;\r\n}\r\n\r\n.search-box{\r\n    top:25px;\r\n    position: absolute;\r\n    background-color: rgb(58, 56, 56);\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content:space-evenly;\r\n    width:400px;\r\n    height:40px;\r\n    z-index: 1;\r\n    border-radius:4px;\r\n}\r\n/* width */\r\n.search-box::-webkit-scrollbar {\r\n    width: 10px;\r\n  }\r\n  \r\n  /* Track */\r\n  .search-box::-webkit-scrollbar-track {\r\n    background: #f1f1f1;\r\n  }\r\n  \r\n  /* Handle */\r\n  .search-box::-webkit-scrollbar-thumb {\r\n    background: rgb(8, 8, 8);\r\n  }\r\n  \r\n  /* Handle on hover */\r\n  .search-box::-webkit-scrollbar-thumb:hover {\r\n    background: rgb(8, 8, 8);\r\n  }\r\n.search-box input{\r\n    width:400px;\r\n    height:40px;\r\n    background:transparent;\r\n    color: rgb(252, 252, 252);\r\n    font-size:1.1em;\r\n    border: none;\r\n    padding-left:10px\r\n}\r\n\r\n.search-box i{\r\n    padding-left:10px;\r\n    cursor: pointer;\r\n    color:white;\r\n}\r\n.country-list{\r\n    display: flex;\r\n    margin-top:10px;\r\n    position: absolute;\r\n    top:60px;\r\n    width:400px;\r\n    height:200px;\r\n    background-color: rgb(31, 30, 30);\r\n    opacity: 0.5;\r\n    color:white;\r\n    font-size:15px;\r\n    overflow-y: scroll;\r\n}\r\n\r\n.country-list ul{\r\n    width:100%;\r\n}\r\n\r\n.country-list ul li{\r\n    list-style: none;\r\n    padding:10px;\r\n    text-align: center;\r\n   \r\n}\r\n.country-list ul li:hover{\r\n    border-left:4px white solid ;\r\n    cursor: pointer;\r\n    transition:1s;\r\n}\r\n\r\n.hide{\r\n    display:none;\r\n}\r\n\r\n\r\n@media (max-width:600px){\r\n    .latest-report{\r\n        margin-top:70px;\r\n        display:flex;\r\n        flex-direction:column;\r\n        align-items: flex-start;\r\n    }\r\n}
